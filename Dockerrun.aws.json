{
	"AWSEBDockerrunVersion": 2,
	"containerDefinitions": [{
			"name": "client",
			"image": "yasir326/multi-client",
			"hostname": "client",
			"essential": false,
		},
		{
			"name": "server",
			"image": "yasir326/multi-server",
			"hostname": "api",
			"essential": false,

		},
		{
			"name": "worker",
			"image": "yasir326/multi-worker",
			"essential": false,
		},
		{
			"name": "nginx",
			"image": "yasir326/multi-nginx",
			"essential": true,
			"portMappings": [{
				"hostport": 80,
				"containerPort": 80
			}],
			"links": ["client", "server"]
		}
	]
}